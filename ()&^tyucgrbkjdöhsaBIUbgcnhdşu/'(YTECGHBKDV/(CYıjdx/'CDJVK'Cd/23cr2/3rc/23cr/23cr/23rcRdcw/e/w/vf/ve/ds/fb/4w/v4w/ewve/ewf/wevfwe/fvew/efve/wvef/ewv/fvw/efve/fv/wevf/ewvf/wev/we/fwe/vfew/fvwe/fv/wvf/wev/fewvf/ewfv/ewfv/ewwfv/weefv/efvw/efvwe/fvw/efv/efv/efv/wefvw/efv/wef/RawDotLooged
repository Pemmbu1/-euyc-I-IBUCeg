local function RGWVOH_fake_script() -- ServiceLogger.LocalScript 
	local script = Instance.new('LocalScript', ServiceLogger)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.e:Destroy()
		wait(0)
		script.Parent.Parent.t:Destroy()
		wait(0)
		script.Parent.Parent.o:Destroy()
		wait(0)
		script.Parent.Parent.u:Destroy()
		wait(0)
		script.Parent.Parent.y:Destroy()
		wait(0)
		script.Parent.Parent.Exit:Destroy()
		wait(0)
		script.Parent.Parent.Wellcome:Destroy()
		wait(0)
		script.Parent.Parent.ServiceLogger.Visible = false
		wait(1)
		script.Parent.Parent.Logger.Visible = true
		wait(3)
		script.Parent.Parent.Logger.A.Visible = true
		wait(2)
		script.Parent.Parent.Logger.B.Visible = true
		wait(7)
		script.Parent.Parent.Logger.C.Visible = true
		wait(3)
		script.Parent.Parent.Logger.D.Visible = true
		wait(3)
		script.Parent.Parent.Logger:Destroy()
		wait(1)
		script.Parent.Parent.Parent.Parent.Parent.Background.Visible = false
		wait(.01)
		game.Lighting.BBTR.Size = 19
		wait(.01)
		game.Lighting.BBTR.Size = 18
		wait(.01)
		game.Lighting.BBTR.Size = 17
		wait(.01)
		game.Lighting.BBTR.Size = 16
		wait(.01)
		game.Lighting.BBTR.Size = 15
		wait(.01)
