local function RBLXSZM_fake_script() -- ServiceLogger.LocalScript 
	local script = Instance.new('LocalScript', ServiceLogger)

	script.Parent.MouseButton1Click:Connect(function()
		wait(4)
		local Message ="A_1"
		local A_1Text ="- Loging J$on Clients.."
		local A_2 ="All"
	
		while true do
			wait(1)
			local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(0.5)
			A_1Text ="- Made By @Pemmbu#6785"
			wait(.01)
			local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(.01)
			A_1Text ="- №Client"
			wait(.01)
			local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(.01)
			A_1Text ="- JobId: "..game.JobId.." | Stable."
			wait(.01)
			local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(.01)
			A_1Text ="- Roblox LocaleId: "..game.LocalizationService.RobloxLocaleId.." | Default."
			wait(.01)
			local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(.01)
			A_1Text ="- System LocaleId: "..game.LocalizationService.SystemLocaleId.." | Default."
			wait(.01)
                        local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(.01)
			A_1Text ="- Repeating Last Steps"
			wait(3)
			local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(.01)
			A_1Text ="- Server [(e4/b1)] is now exploitable!"
			wait(.01)
			local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
			wait(.01)
			A_1Text:Destroy()
			wait(0)
			A_2:Destroy()
		end
	
	end)
end
coroutine.wrap(RBLXSZM_fake_script)()
local function DZWP_fake_script() -- Exit.LocalScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		wait(.01)
		print("• Exiting..")
		wait(.01)
		script.Parent.Parent.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(DZWP_fake_script)()
