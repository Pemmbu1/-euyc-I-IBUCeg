local function IKWQ_fake_script() -- JSONLOGGERV2.Speak 
	local script = Instance.new('LocalScript', JSONLOGGERV2)

	local Message ="A_1"
	local A_1Text ="- Wellcome To J$on Logger!"
	local A_2 ="All"
	
	while true do
		wait(1)
		local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
		wait(0.5)
		A_1Text:Destroy()
		wait(0)
		A_2:Destroy()
	end
	
end
coroutine.wrap(IKWQ_fake_script)()
